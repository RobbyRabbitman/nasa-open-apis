{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nasa-open-apis/shared/types/mrp-types": [
        "libs/shared/types/mrp-types/src/index.ts"
      ],
      "@nasa-open-apis/web/api-rate-limit/api-rate-limit-data": [
        "libs/web/api-rate-limit/api-rate-limit-data/src/index.ts"
      ],
      "@nasa-open-apis/web/api-rate-limit/api-rate-limit-types": [
        "libs/web/api-rate-limit/api-rate-limit-types/src/index.ts"
      ],
      "@nasa-open-apis/web/mrp/features/mrp-explorer-feature": [
        "libs/web/mrp/features/mrp-explorer-feature/src/index.ts"
      ],
      "@nasa-open-apis/web/mrp/mrp-data": ["libs/web/mrp/mrp-data/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
